AUI().use(
	'node',
	function(A) {
		A.one('.input-group-btn > button').on(
			'click', function(event) {
				event.preventDefault();
				var keyword = event.target.text();
				console.log('xxxxx' + keyword);
				A.one('div.form-group.input-text-wrapper > input').set('value', keyword);
			}
		);
	}
);