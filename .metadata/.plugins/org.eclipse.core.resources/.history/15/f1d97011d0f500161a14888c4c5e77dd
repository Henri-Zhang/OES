@ProviderType
public class OESQuestionSoap implements Serializable {
	public static OESQuestionSoap toSoapModel(OESQuestion model) {
		OESQuestionSoap soapModel = new OESQuestionSoap();

		soapModel.setUuid(model.getUuid());
		soapModel.setQuestionId(model.getQuestionId());
		soapModel.setStructrueId(model.getStructrueId());

		return soapModel;
	}

	public static OESQuestionSoap[] toSoapModels(OESQuestion[] models) {
		OESQuestionSoap[] soapModels = new OESQuestionSoap[models.length];

		for (int i = 0; i < models.length; i++) {
			soapModels[i] = toSoapModel(models[i]);
		}

		return soapModels;
	}

	public static OESQuestionSoap[][] toSoapModels(OESQuestion[][] models) {
		OESQuestionSoap[][] soapModels = null;

		if (models.length > 0) {
			soapModels = new OESQuestionSoap[models.length][models[0].length];
		}
		else {
			soapModels = new OESQuestionSoap[0][0];
		}

		for (int i = 0; i < models.length; i++) {
			soapModels[i] = toSoapModels(models[i]);
		}

		return soapModels;
	}

	public static OESQuestionSoap[] toSoapModels(List<OESQuestion> models) {
		List<OESQuestionSoap> soapModels = new ArrayList<OESQuestionSoap>(models.size());

		for (OESQuestion model : models) {
			soapModels.add(toSoapModel(model));
		}

		return soapModels.toArray(new OESQuestionSoap[soapModels.size()]);
	}

