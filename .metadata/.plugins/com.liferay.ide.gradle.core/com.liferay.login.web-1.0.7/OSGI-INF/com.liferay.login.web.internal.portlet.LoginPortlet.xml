  <property name="javax.portlet.name" type="String" value="com_liferay_login_web_portlet_LoginPortlet"/>
  <property name="com.liferay.portlet.use-default-template" type="String" value="true"/>
  <property name="javax.portlet.display-name" type="String" value="Sign In"/>
  <property name="com.liferay.portlet.icon" type="String" value="/icons/login.png"/>
</component>
