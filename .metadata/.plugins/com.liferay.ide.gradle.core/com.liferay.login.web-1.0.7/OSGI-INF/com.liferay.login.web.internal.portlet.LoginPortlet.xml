<?xml version="1.0" encoding="UTF-8"?>
<component name="com.liferay.login.web.internal.portlet.LoginPortlet" immediate="true">
  <implementation class="com.liferay.login.web.internal.portlet.LoginPortlet"/>
  <service>
    <provide interface="javax.portlet.Portlet"/>
  </service>
  <property name="javax.portlet.expiration-cache" type="String" value="0"/>
  <property name="com.liferay.portlet.render-weight" type="String" value="50"/>
  <property name="javax.portlet.security-role-ref" type="String" value="guest,power-user,user"/>
  <property name="javax.portlet.init-param.config-template" type="String" value="/configuration.jsp"/>
  <property name="javax.portlet.init-param.template-path" type="String" value="/"/>
  <property name="com.liferay.portlet.private-session-attributes" type="String" value="false"/>
  <property name="com.liferay.portlet.css-class-wrapper" type="String" value="portlet-login"/>
  <property name="com.liferay.portlet.display-category" type="String" value="category.tools"/>
